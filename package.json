{
  "name": "inunotaisho26.github.io",
  "version": "0.1.0",
  "description": "This is the packagejson for Ethan Fairweather's professional website version 2. This is also MEAN stack in nature. Will probably be written in Angular 2 or JS will be the fallback if need be.",
  "main": "lib/app.js",
  "devDependencies": {
    "babel-preset-env": "^1.6.0",
    "babelify": "^7.3.0",
    "nodemon": "latest",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "angular": "^1.6.5",
    "angular-resource": "^1.6.5",
    "angular-route": "^1.6.5",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.1",
    "bootstrap": "^3.3.7",
    "cluster": "^0.7.7",
    "concurrently": "^3.4.0",
    "connect-redis": "^3.2.0",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "font-awesome": "^4.7.0",
    "helmet": "^3.6.1",
    "jquery": "^3.2.1",
    "jspm": "^0.16.53",
    "less": "^2.7.2",
    "less-plugin-clean-css": "^1.5.1",
    "lodash": "^4.17.4",
    "minifyify": "^7.3.5",
    "mkdirp": "^0.5.1",
    "nodemailer": "^4.0.1",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0",
    "stringify": "^5.1.0"
  },
  "scripts": {
    "babel": "babel -o src/app/main.js > src/app/app.js",
    "browserify": "browserify src/app/app.js -d -t stringify -o src/dist/app.js -v -p [minifyify --no-map]",
    "bundle": "mkdirp src/dist && concurrently -r -k \"npm run babel\" \"npm run browserify \" \" npm run less\"",
    "clean": "rimraf app/**/*.css app/src/**/*.js app/dist/*",
    "clean:dist": "rimraf app/dist/*",
    "less": "mkdirp public/dist && lessc ./src/styles/main.less > ./public/dist/app.css --clean-css=\"--s0 --advanced\"",
    "dev": "NODE_ENV=development nodemon ./lib/app.js",
    "prestart": "sequelize db:migrate && npm run bundle",
    "start": "nodemon ./lib/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inunotaisho26/inunotaisho26.github.com.git"
  },
  "keywords": [
    "example",
    "fullstack",
    "postgres",
    "inunotaisho26",
    "犬の大将",
    "晴天イタン"
  ],
  "author": "Ethan Burrow Fairweather",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/inunotaisho26/inunotaisho26.github.com/issues"
  },
